{
  "framework": "vite",
  "buildCommand": "cd fartcaster && npm install && npm run build",
  "devCommand": "cd fartcaster && npm run dev",
  "outputDirectory": "fartcaster/dist",
  "installCommand": "cd fartcaster && npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
